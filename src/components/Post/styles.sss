.post
	width: 480px
	background-color: #fff
	border: 1px solid #e6e6e6
	border-radius: 3px

.post__info
	padding: 0 16px 20px

.photo
	width: 100%
	height: auto

svg.placeholder
	max-width: 100%
	width: 100%
	height: 400px
	background: url("data:image/svg+xml;base64,") no-repeat center hsl(0, 0%, 80%)
	background-size: calc(100%/3)

.info__line
	padding-bottom: 8px

.header
	box-sizing: border-box
	display: flex
	height: 60px
	width: 100%
	align-items: center
	border-top: 0px solid rgb(0, 0, 0)
	border-right: 0px solid rgb(0, 0, 0)
	border-bottom: 0.5px solid rgb(239, 239, 239)
	border-left: 0px solid rgb(0, 0, 0)
	flex: 0 0 auto
	padding: 16px

.avatar
	box-sizing: border-box
	display: flex
	height: 30px
	position: relative
	right: 0px
	text-size-adjust: 100%
	top: 0px
	width: 30px
	align-items: center
	justify-content: center
	border: 0px solid rgb(0, 0, 0)
	flex: 0 0 auto
	flex-flow: column nowrap
	font: normal normal normal normal 14px / 18px -apple-system, system-ui, "Segoe UI", Roboto, Helvetica, Arial, sans-serif

.image__wrapper
	box-sizing: border-box
	color: rgb(0, 53, 105)
	cursor: pointer
	display: block
	height: 30px
	text-decoration: none solid rgb(0, 53, 105)
	text-size-adjust: 100%
	top: 0px
	width: 30px
	background: rgb(250, 250, 250) none repeat scroll 0% 0% / auto padding-box border-box
	border-radius: 50% 50% 50% 50%
	flex: 0 0 auto
	font: normal normal normal normal 14px / 18px -apple-system, system-ui, "Segoe UI", Roboto, Helvetica, Arial, sans-serif
	outline: rgb(0, 53, 105) none 0px
	overflow: hidden

.image__wrapper:after
	color: rgb(0, 53, 105)
	content: ''
	cursor: pointer
	display: block
	height: 28px
	left: 0px
	pointer-events: none
	position: absolute
	right: 0px
	text-decoration: none solid rgb(0, 53, 105)
	text-size-adjust: 100%
	top: 0px
	width: 28px
	border: 1px solid rgba(0, 0, 0, 0.098)
	border-radius: 50% 50% 50% 50%
	font: normal normal normal normal 14px / 18px -apple-system, system-ui, "Segoe UI", Roboto, Helvetica, Arial, sans-serif
	outline: rgb(0, 53, 105) none 0px

.image
	color: rgb(0, 53, 105)
	cursor: pointer
	height: 30px
	text-decoration: none solid rgb(0, 53, 105)
	width: 30px
	font: normal normal normal normal 14px / 18px -apple-system, system-ui, "Segoe UI", Roboto, Helvetica, Arial, sans-serif
	outline: rgb(0, 53, 105) none 0px


.text__wrap
	box-sizing: border-box
	display: flex
	height: 18px
	align-items: flex-start
	perspective-origin: 263px 9px
	transform-origin: 263px 9px
	flex: 1 1 auto
	font: normal normal normal normal 14px / 18px -apple-system, system-ui, "Segoe UI", Roboto, Helvetica, Arial, sans-serif
	margin: 0px 0px 0px 12px
	overflow: hidden

.text
	color: rgb(38, 38, 38)
	height: 18px
	font-size: 16px !important
	font: normal normal normal normal 14px / 18px -apple-system, system-ui, "Segoe UI", Roboto, Helvetica, Arial, sans-serif

	&_bold
		font-weight: 600

.icons
	box-sizing: border-box
	display: flex
	height: 40px
	text-size-adjust: 100%
	top: 0px
	align-items: stretch
	perspective-origin: 284px 20px
	transform-origin: 284px 20px
	border: 0px solid rgb(0, 0, 0)
	flex: 0 0 auto
	font: normal normal normal normal 14px / 18px -apple-system, system-ui, "Segoe UI", Roboto, Helvetica, Arial, sans-serif
	margin: 4px 0px 0px

.icon
	color: rgb(0, 53, 105)
	cursor: pointer
	display: block
	height: 24px
	text-decoration: none solid rgb(0, 53, 105)
	text-size-adjust: 100%
	width: 24px
	font: normal normal normal normal 14px / 18px -apple-system, system-ui, "Segoe UI", Roboto, Helvetica, Arial, sans-serif
	outline: rgb(0, 53, 105) none 0px
	padding: 8px 16px 8px 0

.icon_last
	padding-right: 0
	margin-left: auto

.icon_like
	color: rgb(0, 53, 105)
	cursor: pointer
	display: block
	height: 24px
	width: 24px
	background-image: url("https://www.instagram.com/static/bundles/sprite_core_2x.png/d642691e37e2.png")
	background-position: -50px -377px
	background-size: 429px 401px

